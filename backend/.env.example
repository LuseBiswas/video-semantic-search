# Supabase Postgres
DATABASE_URL=postgresql+psycopg://<USER>:<PASSWORD>@<HOST>:5432/<DBNAME>

# Supabase service creds (server-side only; never ship to frontend)
SUPABASE_URL=https://<your-project-id>.supabase.co
SUPABASE_SERVICE_ROLE_KEY=sbp-<service-role-key>

# Storage buckets (name them in Supabase)
BUCKET_VIDEOS=videos
BUCKET_FRAMES=frames
BUCKET_PREVIEWS=previews

# Model settings
MODEL_NAME=ViT-B-32
MODEL_PRETRAIN=openai
EMB_DIM=768

# CORS for your Next.js app
CORS_ORIGINS=http://localhost:3000
